{
	"name": "node-unrtf",
	"version": "1.0.6",
	"description": "Asynchronous node.js wrapper for the UnRTF RTF conversion program",
	"keywords": [
		"async",
		"converter",
		"html",
		"latex",
		"ps",
		"rtf",
		"rtf-converter",
		"rtf-to-html",
		"rtf-to-latex",
		"rtf-to-ps",
		"rtf-to-text",
		"rtf-to-wpml",
		"text",
		"txt",
		"unrtf",
		"wpml"
	],
	"main": "src/index.js",
	"repository": "git+https://github.com/Fdawgs/node-unrtf.git",
	"author": "Frazer Smith",
	"license": "MIT",
	"engines": {
		"node": ">=10.22.0"
	},
	"scripts": {
		"changelog": "conventional-changelog -p @commitlint/config-conventional -s -i CHANGELOG.md && prettier CHANGELOG.md --write",
		"doc": "tsc && jsdoc2md src/index.js > API.md && prettier API.md --write",
		"dupe-check": "yarn jsinspect ./src",
		"jest": "jest --detectOpenHandles --runInBand",
		"jest-coverage": "jest --coverage --runInBand",
		"lint": "eslint . --cache --ext js,jsx --ignore-path .gitignore",
		"lint:prettier": "prettier . --write --ignore-path .gitignore",
		"lint:prettier:ci": "prettier . --check --ignore-path .gitignore",
		"test": "yarn lint && yarn jest"
	},
	"jest": {
		"clearMocks": true,
		"testEnvironment": "node",
		"testTimeout": 10000
	},
	"prettier": {
		"endOfLine": "lf",
		"semi": true,
		"tabWidth": 4,
		"useTabs": true
	},
	"devDependencies": {
		"@commitlint/config-conventional": "^11.0.0",
		"conventional-changelog-cli": "^2.1.1",
		"coveralls": "^3.1.0",
		"eslint": "^7.17.0",
		"eslint-config-airbnb-base": "^14.2.1",
		"eslint-config-prettier": "^7.1.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jest": "^24.1.3",
		"eslint-plugin-jsdoc": "^30.7.13",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-security": "^1.4.0",
		"is-html": "^2.0.0",
		"jest": "^26.6.3",
		"jsdoc-to-markdown": "^6.0.1",
		"jsinspect": "^0.12.7",
		"prettier": "2.2.1",
		"typescript": "^4.1.3"
	}
}
